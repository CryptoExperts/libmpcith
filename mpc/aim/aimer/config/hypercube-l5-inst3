export LIBMPCITH_SCHEME_PATH=aim/aimer/aim256
export LIBMPCITH_ARITH_LIBS=
export LIBMPCITH_CONFIG_FLAGS=HYPERCUBE_5R,L5,INST3,RND_EXPANSION_X4
export LIBMPCITH_DRBG_PATH=hash
export LIBMPCITH_HASH_PATH=sha3/sha3-several
export LIBMPCITH_HASH_SOURCE_FOLDER=sha3/sha3-several/sha3
export LIBMPCITH_HASH_SOURCE_INCLUDE=avx2
export LIBMPCITH_HASH_MAKE_OPTIONS=PLATFORM=avx2
export LIBMPCITH_ADD_FLAGS=HASHX4,XOFX4,PRGX4
# NOTE: since aim uses endian.h, GNU extensions must
# be activated using "-std=gnu11"
export LIBMPCITH_COMPILATION_FLAGS=-mavx,-std=gnu11
